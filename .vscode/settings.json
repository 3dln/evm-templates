{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.cache": true,
        "**/.vs/": true,
        "**/*.cs": true,
        "**/*.orig": true,
        "**/bin/": true,
        "**/build/": true,
        "**/debug/": true,
        "**/dist/": true,
        "**/node_modules/": true,
        "**/obj": true,
